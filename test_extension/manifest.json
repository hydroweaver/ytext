{
    "name": "Hello Extensions Test",
    "description" : "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action" : {
	"default_popup" : "index.html",
	"default_icon" : "ytext.png"
    },
    "background" :{
        "scripts" : ["index.js"]
    },
    "commands" : {
        "_execute_browser_action" : {
            "suggested_key" : {
                "default" : "Ctrl+Shift+F",
                "mac" : "MacCtrl+Shift+F"
            },
            "description" : "Opens index.html"
        }
    }
 }